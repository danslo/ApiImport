<?xml version="1.0" encoding="UTF-8"?>
<config>
    <api>
        <resources>
            <import translate="title" module="api_import">
                <model>api_import/import_api</model>
                <title>Import API</title>
                <acl>api_import</acl>
                <methods>
                    <importEntities translate="title" module="api_import">
                        <title>Import entities</title>
                    </importEntities>
                </methods>
                <faults module="api_import">
                    <invalid_entity_type>
                        <code>100</code>
                        <message>Entity type is not valid.</message>
                    </invalid_entity_type>
                    <invalid_behavior>
                        <code>101</code>
                        <message>Import behavior is not valid.</message>
                    </invalid_behavior>
                    <import_failed>
                        <code>102</code>
                        <message>Import failed. Please see message for more information.</message>
                    </import_failed>
                </faults>
            </import>
        </resources>
        <v2>
            <resources_function_prefix>
                <import>apiImport</import>
            </resources_function_prefix>
        </v2>
        <acl>
            <resources>
                <api_import translate="title" module="api_import">
                    <title>API Import</title>
                    <sort_order>1</sort_order>
                    <importEntities translate="title" module="api_import">
                        <title>Import entities</title>
                    </importEntities>
                </api_import>
            </resources>
        </acl>
    </api>
</config>